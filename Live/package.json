{
  "name": "live",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng build l3as && ng serve",
    "start:app:admin": "ng serve admin --port 4200 --host 0.0.0.0",
    "start:app:client": "ng serve client --port 4200 --host 0.0.0.0",
    "start:app:service": "ng serve service",
    "clean": "rimraf dist",
    "get-version": "ts-node -O \"{\\\"module\\\": \\\"commonjs\\\"}\" git-version.ts",
    "build": "npm-run-all clean get-version build:**",
    "build-prod": "npm-run-all clean get-version \"build:** -- --prod\"",
    "build:lib:l3as": "ng build l3as",
    "build:app:admin": "ng build admin",
    "build:app:client": "ng build client",
    "build:app:service": "ng build service",
    "test": "npm-run-all get-version test:**",
    "test:lib:l3as": "ng test l3as",
    "test:app:admin": "ng test admin",
    "test:app:client": "ng test client",
    "test:app:service": "ng test service",
    "apitests": "node_modules/.bin/newman run apps/service-apitests/live-server-apitests-postman-collection.json -e apps/service-apitests/localhost-3000.postman-environment.json",
    "lint": "./node_modules/.bin/nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "pkg": "pkg ./dist/apps/service/package.json -t node10-win --out-path pkg --debug > pkg-debug.log",
    "bundle": "ncp pkg bundle && ncp dist/apps/client bundle/client && ncp dist/apps/admin bundle/admin && ncp misc/bundle-structure bundle",
    "copy-ffmpeg": "ncp node_modules/ffmpeg-static/bin/win32/x64/ bundle/ffmpeg",
    "build-exe": "npm-run-all pkg bundle copy-ffmpeg",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:libs": "./node_modules/.bin/nx affected:libs",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "affected:lint": "./node_modules/.bin/nx affected:lint",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "affected": "./node_modules/.bin/nx affected",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "help": "./node_modules/.bin/nx help",
    "nx": "nx"
  },
