{
  "name": "live",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng build l3as && ng serve",
    "start:app:admin": "ng serve admin --port 4200 --host 0.0.0.0",
    "start:app:client": "ng serve client --port 4200 --host 0.0.0.0",
    "start:app:service": "ng serve service",
    "clean": "rimraf dist",
    "get-version": "ts-node -O '{\"module\": \"commonjs\"}' git-version.ts",
    "build": "npm-run-all clean get-version build:**",
    "build:lib:l3as": "ng build l3as --prod",
    "build:app:admin": "ng build admin --prod",
    "build:app:client": "ng build client --prod",
    "build:app:service": "ng build service --prod",
    "test": "npm-run-all get-version test:**",
    "test:lib:l3as": "ng test l3as",
    "test:app:admin": "ng test admin",
    "test:app:client": "ng test client",
    "test:app:service": "ng test service",
    "lint": "./node_modules/.bin/nx lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:libs": "./node_modules/.bin/nx affected:libs",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:e2e": "./node_modules/.bin/nx affected:e2e",
    "affected:test": "./node_modules/.bin/nx affected:test",
    "affected:lint": "./node_modules/.bin/nx affected:lint",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "affected": "./node_modules/.bin/nx affected",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "ng update @nrwl/schematics",
    "update:check": "ng update",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "help": "./node_modules/.bin/nx help"
  },
  "dependencies": {
    "@angular/animations": "^7.2.15",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "^7.2.15",
    "@angular/compiler": "^7.2.15",
    "@angular/core": "^7.2.15",
    "@angular/forms": "^7.2.15",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "^7.2.15",
    "@angular/platform-browser-dynamic": "^7.2.15",
    "@angular/router": "^7.2.15",
    "core-js": "^2.5.4",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "inversify": "^5.0.1",
    "inversify-express-utils": "^6.2.0",
    "logform": "^1.10.0",
    "ng-inline-svg": "^8.1.0",
    "ng5-slider": "^1.1.14",
    "node-schedule": "^1.3.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "6.5.2",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "winston": "^3.1.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.9",
    "@angular/cli": "^7.3.9",
    "@angular/compiler-cli": "^7.2.15",
    "@angular/language-service": "^7.2.15",
    "@nrwl/builders": "^7.8.5",
    "@nrwl/schematics": "^7.7.1",
    "@types/app-root-path": "^1.2.4",
    "@types/express": "4.16.0",
    "@types/jest": "24.0.9",
    "@types/jquery": "3.3.6",
    "@types/logform": "^1.2.0",
    "@types/node": "~8.9.4",
    "@types/node-schedule": "^1.2.2",
    "@types/socket.io": "^2.1.2",
    "app-root-path": "^2.1.0",
    "codelyzer": "~4.5.0",
    "cypress": "^3.2.0",
    "jest": "^24.8.0",
    "jest-create-mock-instance": "^1.1.0",
    "jest-preset-angular": "^7.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "3.2.4"
  }
}