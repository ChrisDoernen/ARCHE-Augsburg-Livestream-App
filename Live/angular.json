{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "client": {
      "root": "apps/client/",
      "sourceRoot": "apps/client/src",
      "projectType": "application",
      "prefix": "live",
      "schematics": {
        "@nrwl/schematics:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/client",
            "index": "apps/client/src/index.html",
            "main": "apps/client/src/main.ts",
            "polyfills": "apps/client/src/polyfills.ts",
            "tsConfig": "apps/client/tsconfig.app.json",
            "assets": [
              "apps/client/src/favicon.ico",
              "apps/client/src/manifest.json",
              "apps/client/src/assets",
              "apps/client/src/manifest.json"
            ],
            "styles": [
              "apps/client/src/styles.scss"
            ],
            "scripts": [
              "apps/client/src/assets/js/pwacompat.min.js"
            ],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/client/src/environments/environment.ts",
                  "with": "apps/client/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ],
              "serviceWorker": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "client:build",
            "proxyConfig": "apps/client/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "client:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "client:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/client/tsconfig.app.json",
              "apps/client/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/client/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/client/jest.config.js",
            "tsConfig": "apps/client/tsconfig.spec.json",
            "setupFile": "apps/client/src/test-setup.ts"
          }
        }
      }
    },
    "client-e2e": {
      "root": "apps/client-e2e",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/client-e2e/cypress.json",
            "tsConfig": "apps/client-e2e/tsconfig.e2e.json",
            "devServerTarget": "client:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "client:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/client-e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**",
              "!apps/client-e2e/**"
            ]
          }
        }
      }
    },
    "service": {
      "root": "apps/service",
      "sourceRoot": "apps/service/src",
      "projectType": "application",
      "prefix": "service",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "webpackConfig": "apps/service/webpack.config.js",
            "outputPath": "dist/apps/service",
            "filename": "live.js",
            "main": "apps/service/src/live.ts",
            "tsConfig": "apps/service/tsconfig.app.json",
            "assets": [
              "apps/service/src/data"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/service/src/environments/environment.ts",
                  "with": "apps/service/src/environments/environment.prod.ts"
                }
              ],
              "externalDependencies": "all",
              "sourceMap": false
            },
            "simulation": {
              "fileReplacements": [
                {
                  "replace": "apps/service/src/environments/environment.ts",
                  "with": "apps/service/src/environments/environment.simulation.ts"
                }
              ]
            },
            "filesource": {
              "fileReplacements": [
                {
                  "replace": "apps/service/src/environments/environment.ts",
                  "with": "apps/service/src/environments/environment.filesource.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "service:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "service:build:production"
            },
            "simulation": {
              "buildTarget": "service:build:simulation"
            },
            "filesource": {
              "buildTarget": "service:build:filesource"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/service/tsconfig.app.json",
              "apps/service/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/service/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/service/jest.config.js",
            "tsConfig": "apps/service/tsconfig.spec.json"
          }
        }
      }
    },
    "entities": {
      "root": "libs/entities",
      "sourceRoot": "libs/entities/src",
      "projectType": "library",
      "prefix": "live",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/entities/tsconfig.lib.json",
              "libs/entities/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/entities/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/entities/jest.config.js",
            "tsConfig": "libs/entities/tsconfig.spec.json"
          }
        }
      },
      "schematics": {
        "@nrwl/schematics:component": {
          "styleext": "scss"
        }
      }
    },
    "l3as": {
      "root": "libs/l3as",
      "sourceRoot": "libs/l3as/src",
      "projectType": "library",
      "prefix": "live",
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/libs/l3as",
            "main": "libs/l3as/src/index.js",
            "tsConfig": "libs/l3as/tsconfig.lib.json",
            "project": "libs/l3as/package.json"
          },
          "configurations": {
            "production": {
              "optimization": true,
              "sourceMap": false
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/l3as/tsconfig.lib.json",
              "libs/l3as/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/l3as/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/l3as/jest.config.js",
            "tsConfig": "libs/l3as/tsconfig.spec.json"
          }
        }
      },
      "schematics": {
        "@nrwl/schematics:component": {
          "styleext": "scss"
        }
      }
    },
    "services": {
      "root": "libs/services",
      "sourceRoot": "libs/services/src",
      "projectType": "library",
      "prefix": "live",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/services/tsconfig.lib.json",
              "libs/services/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/services/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/services/jest.config.js",
            "tsConfig": "libs/services/tsconfig.spec.json",
            "setupFile": "libs/services/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/schematics:component": {
          "styleext": "scss"
        }
      }
    },
    "admin": {
      "root": "apps/admin/",
      "sourceRoot": "apps/admin/src",
      "projectType": "application",
      "prefix": "live",
      "schematics": {
        "@nrwl/schematics:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/admin",
            "index": "apps/admin/src/index.html",
            "baseHref": "/admin/",
            "deployUrl": "/admin/",
            "main": "apps/admin/src/main.ts",
            "polyfills": "apps/admin/src/polyfills.ts",
            "tsConfig": "apps/admin/tsconfig.app.json",
            "assets": [
              "apps/admin/src/favicon.ico",
              "apps/admin/src/assets",
              "apps/admin/src/manifest.json",
              {
                "glob": "**/*",
                "input": "./node_modules/material-icons/iconfont/",
                "output": "./assets/iconfont"
              }
            ],
            "styles": [
              "apps/admin/src/styles.scss"
            ],
            "scripts": [],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/admin/src/environments/environment.ts",
                  "with": "apps/admin/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ],
              "serviceWorker": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "admin:build",
            "proxyConfig": "apps/client/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "admin:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "admin:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/admin/tsconfig.app.json",
              "apps/admin/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/admin/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/admin/jest.config.js",
            "tsConfig": "apps/admin/tsconfig.spec.json",
            "setupFile": "apps/admin/src/test-setup.ts"
          }
        }
      }
    },
    "admin-e2e": {
      "root": "apps/admin-e2e",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/admin-e2e/cypress.json",
            "tsConfig": "apps/admin-e2e/tsconfig.e2e.json",
            "devServerTarget": "admin:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "admin:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/admin-e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**",
              "!apps/admin-e2e/**"
            ]
          }
        }
      }
    },
    "constants": {
      "root": "libs/constants",
      "sourceRoot": "libs/constants/src",
      "projectType": "library",
      "prefix": "live",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/constants/tsconfig.lib.json",
              "libs/constants/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/constants/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/constants/jest.config.js",
            "tsConfig": "libs/constants/tsconfig.spec.json"
          }
        }
      },
      "schematics": {
        "@nrwl/schematics:component": {
          "styleext": "scss"
        }
      }
    },
    "test-utilities": {
      "root": "libs/test-utilities",
      "sourceRoot": "libs/test-utilities/src",
      "projectType": "library",
      "prefix": "live",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/test-utilities/tsconfig.lib.json",
              "libs/test-utilities/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/test-utilities/**"
            ]
          }
        }
      },
      "schematics": {
        "@nrwl/schematics:component": {
          "styleext": "scss"
        }
      }
    }
  },
  "cli": {
    "warnings": {
      "typescriptMismatch": false,
      "versionMismatch": false
    },
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/schematics:application": {
      "style": "scss"
    },
    "@nrwl/schematics:library": {
      "style": "scss"
    }
  },
  "defaultProject": "client"
}